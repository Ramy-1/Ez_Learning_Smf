{% extends 'base.html.twig' %}

{% block title %}Universite{% endblock %}

{% block body %}
<div class="row">
    <div  style="display: list-item;align-items: normal;justify-content: normal;margin-top: 15px; padding: 10px;">

        <div class="card">
            <div class="card-body">
    <h1 class="">Universite</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Iduni</th>
                <td>{{ universite.iduni }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ universite.nom }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ universite.email }}</td>
            </tr>
            <tr>
                <th>Adresse</th>
                <td>{{ universite.adresse }}</td>
            </tr>
            <tr>
                <th>Imguni</th>
                <td>{{ universite.imguni }}</td>
            </tr>
            <tr>
                <th>Mdpuni</th>
                <td>{{ universite.mdpuni }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_universite_index') }}" class="btn-success">back to list</a>

    <a href="{{ path('app_universite_edit', {'iduni': universite.iduni}) }}" class="btn-success">edit</a>

    {{ include('universite/_delete_form.html.twig') }}
{% endblock %}
